<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidtutorialshub.loginregister">

    <application
        android:allowBackup="true"
        android:icon="@drawable/iecse_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.RegisterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.UsersListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.FillEvent"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.ViewEvent"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.FillAttendance"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.ViewAttendance"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.FillRegistration"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.ViewRegistration"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.FillMember"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.ViewMember"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.FillNonMember"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.ViewNonMember"
            android:screenOrientation="portrait" />

    </application>

</manifest>